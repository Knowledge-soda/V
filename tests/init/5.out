_start:
    NAME(id) BEGIN END
--:
    BEGIN NAME(var) END : NAME(=) BEGIN NAME(var) SEP NAME(+) BEGIN NAME(v) BEGIN NAME(var) END SEP NUM(-1) END END
++:
    BEGIN NAME(var) END : NAME(=) BEGIN NAME(var) SEP NAME(+) BEGIN NAME(v) BEGIN NAME(var) END SEP NUM(1) END END
+=:
    BEGIN NAME(var) SEP NAME(n) END : NAME(=) BEGIN NAME(var) SEP NAME(+) BEGIN NAME(v) BEGIN NAME(var) END SEP NAME(n) END END
sum:
    BEGIN NAME(x) END : NAME(v) BEGIN NAME(=) BEGIN NAME(b) SEP NUM(0) END NAME(=) BEGIN NAME(a) SEP NUM(0) END PLACE(rep) NAME(++) BEGIN NAME(b) END NAME(+=) BEGIN NAME(a) SEP NAME(v) BEGIN NAME(b) END END NAME(ijmp) BEGIN NAME(<) BEGIN NAME(v) BEGIN NAME(b) END SEP NAME(x) END SEP NAME(rep) END NAME(a) END
